<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width,initial-scale=1,maximum-scale=1,shrink-to-fit=no" />
    <meta name="renderer" content="webkit" />
    <meta name="force-rendering" content="webkit" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="google-site-verification" content="KlgPm2y_TrkYFQ85akszzF094bFUFjgXpby16b2BAXE" />
    <link rel="apple-touch-icon" sizes="180x180" href="/images/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/images/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/images/favicon-16x16.png">
    <link rel="icon" type="image/png" sizes="48x48" href="/images/favicon.ico">
    <title><%= config.title %></title>
    <script type="application/ld+json">
      {
        "@context": "https://schema.org",
        "@type": "WebSite",
        "name": "<%= config.title %>",
        "url": "https://www.eternalwings.xyz"
      }
      </script>
    <%- partial('partials/import-css.ejs') %>
    <%- partial('partials/import-svg.ejs') %>
  </head>
  <body class="mdui-theme-layout-light mdui-loaded mdui-theme-primary-deep-purple mdui-theme-accent-deep-purple">
    <!-- 侧边栏 -->
    <%- partial('partials/navbar.ejs') %>
    <% if (is_home_first_page()) { %>
    <!-- 主页图片 -->
    <div class="homepage">
      <img class="thumbnail" style="opacity: 0;" onload="this.style.opacity=1;" data-blurhash="L59IC+0000.TYRW9rWtoT2Wqxnem" data-width="32" data-height="16">
      <video src="/images/Eve 自宅雑談_2_prob4.mp4" muted loop autoplay oncanplaythrough="this.style.opacity=1" class="background"></video>
      <div class="info">
        <div class="author"><%= config.author %>的博客</div>
        <div class="signature"><%= config.signature %></div>
      </div>
    </div>
    <% } %>
    <div class="mdui-container mdui-typo">
      <%- partial('partials/fixed-user-info.ejs') %>
      <!-- 内容区域 -->
      <div class="mdui-content"><%- body %></div>
    </div>
    <!-- 悬浮按钮 -->
     <div class="fixed-btns">
       <div class="upBtn" onclick="window.scrollTo({top: 0, behavior: 'smooth'});"></div>
     </div>
    <!-- 引入JS -->
    <%- partial('partials/import-js.ejs') %>
    <script src="//instant.page/5.2.0" type="module" integrity="sha384-jnZyxPjiipYXnSU0ygqeac2q7CVYMbh84q0uHVRRxEtvFPiQYbXWUorga2aqZJ0z"></script>
  </body>
</html>
