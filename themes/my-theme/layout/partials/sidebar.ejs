<div class="mdui-drawer" id="drawer">
  <ul class="mdui-list">
    <% Object.keys(theme.sidebar).forEach(function(key) { %>
    <a class="mdui-list-item mdui-ripple" href="<%= theme.sidebar[key].href %>">
      <i class="mdui-list-item-icon mdui-icon material-icons">
        <svg width="24" height="24" fill="#e3e3e3">
          <use href="#<%= theme.sidebar[key].icon %>"></use>
        </svg>
      </i>
      <div class="mdui-list-item-content"><%= key %></div>
    </a>
    <% }) %>
  </ul>
</div>
